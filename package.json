{
  "name": "igcs-keyterms",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "check:language": "node scripts/check_english.js --paths=src,scripts,public,package.json,vercel.json,tsconfig.app.json,tsconfig.node.json",
    "normalize:papers": "node scripts/normalizePaperCodes.mjs",
    "convert:paper2": "node scripts/convertPaper2.mjs",
    "convert:paper1": "node scripts/convertPaper1.mjs",
    "extract": "node scripts/extract_requirements.js --input=./input.pdf --output=./output",
    "validate": "node scripts/validate_syllabus.js --schema=./src/data/syllabus.schema.json",
    "pipeline": "npm run extract && npm run validate -- --strict",
    "test": "jest"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^3.0.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "joi": "^17.12.0",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^4.7.392",
    "sass": "^1.97.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4"
  }
}
